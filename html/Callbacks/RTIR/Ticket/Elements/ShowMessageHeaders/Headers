<%init>
return if $display_headers{'_all'};
my $i = 0;
while ($i < scalar @headers) {
    $i++;
    next unless ($display_headers{lc($headers[$i-1]->{'Tag'})});
    $m->comp('/RTIR/Elements/MakeClicky', content => \$headers[$i-1]->{'Value'});
}
</%init>
<%ARGS>
$Transaction => undef
$content => undef
%display_headers => undef
@headers => undef
</%ARGS>
