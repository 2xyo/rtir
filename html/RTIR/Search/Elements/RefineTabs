<& '/RTIR/'.$Type.'/Elements/Tabs', 
    subtabs => $refinetabs,
    %ARGS
&>

<%INIT>
my ($Type) = $m->comp("/RTIR/Elements/Type", Queue => $Queue);

my $refinetabs = {
    _a => {
        class => "nav",
        path  => delete $ARGS{'path'},
        title => loc('Refine'),
    },
};
</%INIT>

<%ARGS>
$Queue => ''
</%ARGS>
