<& '/RTIR/'.$Type.'/Elements/Tabs', 
    subtabs => $refinetabs,
    %ARGS
&>

<%INIT>
my ($Type) = $m->comp("/RTIR/Elements/Type", Queue => $Queue);

my $refinetabs = { };
$refinetabs->{'_a'} = {
    class => "nav",
    path  => $path,
    title => loc('Refine'),
};
</%INIT>
	
<%ARGS>
$path => undef
$Queue => undef
</%ARGS>
